<div class="row ">
  <div class="col-lg-12">
    <div id="opening_inventory">
      <table class="table table-bordered" style="background: white;">
        <thead>
          <tr>
            <td colspan="2" class="m_header">
              <b>OPENING INVENTORY</b>

            </td>

            <td>Tank Level (%)</td>
            <th>08-12-2018<br>Sat</th>
            <th>08-12-2018<br>Sat</th>
            <th>08-12-2018<br>Sat</th>
            <th>08-12-2018<br>Sat</th>
            <th>08-12-2018<br>Sat</th>
            <th>08-12-2018<br>Sat</th>
            <th>08-12-2018<br>Sat</th>
          </tr>
        </thead>
        <tbody>
          <% @pro_name.each do |product| %>
          <tr class="<%= product.name.downcase.to_s %>_tr" style="display: table-row;">
            <td rowspan="<%= product.tanks.count + 1 %>" class="col-header" id="<%= product.name.downcase.to_s %>_header" onclick="<%= product.name.downcase.to_s %>_fun();" style="font-weight: bold; color: rgb(68, 68, 68);">
              <%= product.name %>
            </td>
            <td style="background: rgb(247, 255, 247); text-align: right;">total</td>

            <td></td>
            <td>t_value</td>
          </tr>
          <% product.tanks.each do |tank| %>
          <tr class="<%= product.name.downcase.to_s %>_tr" style="display: table-row;">

                  <td><%= tank.name %></td>
                  <td><%= tank.tank_capacity %></td>
                  <td>value</td>
            </tr>
                <% end %>
          <% end %>
          <tr class="control_tr">
            <td colspan="10">
              <div style="text-align: right;">
                <button value="E" class="btn btn-small btn-white" onclick="expand_all()">Expand All</button>
                <button value="E" class="btn btn-small btn-white" onclick="collapse_all()">Collapse All</button>
              </div>

            </td>
          </tr>
        </tbody>
      </table>
    </div>
